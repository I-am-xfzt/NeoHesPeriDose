{
  "name": "neohes-peridose",
  "version": "1.0.0",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "start:server": "cd koa-file-explorer && npm start",
    "start:web": "cd web-ui && pnpm dev",
    "dev": "npm-run-all --parallel start:*",
    "i:server": "cd koa-file-explorer && npm install",
    "i:web": "cd web-ui && pnpm install",
    "install:all": "npm-run-all --parallel i:*"
  }
}
